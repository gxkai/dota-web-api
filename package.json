{
  "name": "dota2_web_api",
  "version": "1.2.2",
  "description": "ES6 Wrapper for the DotA 2 Web API",
  "main": "./src/index.js",
  "scripts": {
    "start": "babel-node --presets es2015 src/api.js",
    "build": "babel src -d lib",
    "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "test": "mocha ./lib/api.test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vilst3r/dota2-web-api.git"
  },
  "keywords": [
    "dota2",
    "web",
    "api",
    "steam"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^4.0.1"
  },
  "author": "Clifford Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vilst3r/dota2-web-api/issues"
  },
  "homepage": "https://github.com/vilst3r/dota2-web-api#readme",
  "dependencies": {
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1"
  }
}
